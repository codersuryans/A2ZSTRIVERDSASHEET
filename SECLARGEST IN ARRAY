public class SECL {
    public static void main(String [] args){
        int a[]={1,2,4,7,7,5};
        int lar=a[0];
        int slar=-1;
        for(int i=0;i<a.length;i++){
            if(a[0]<a[i]){
                slar=lar;
                lar=a[i];
    }
        
            else{
               
                    if(a[i]>slar && a[i]<lar){
                        slar=a[i];
                    }
              
            }
        }
    }
}
        
