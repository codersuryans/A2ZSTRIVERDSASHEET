public class insertionsort {
    public static void main(String args[]){
        int a[]={5,1,6,2,4,3};
        int tem,j;
        for(int i=0;i<a.length;i++){
            tem=a[i];
            j=i;
            while(j>0&&a[j-1]>tem){
                a[j]=a[j-1];
                j--;
            }
            a[j]=tem;
        }
    }
}
